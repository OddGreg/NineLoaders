{
    "name"             : "oddgreg/nine-loaders",
    "description"      : "Configuration and Loading Classes.",
    "type"             : "library",
    "license"          : "MIT",
    "authors"          : [
        {
            "name" : "Greg Truesdell",
            "email": "odd.greg@gmail.com"
        }
    ],
    "autoload"         : {
        "classmap"             : [
            "tests",
            "src"
        ],
        "files"                : [
            "src/Support/helpers.php"
        ],
        "exclude-from-classmap": [
        ],
        "psr-4"                : {
            "Nine\\Loaders\\": "src"
        }
    },
    "minimum-stability": "dev",
    "require"          : {
        "oddgreg/nine-library"               : "dev-master",
        "container-interop/container-interop": "^1.1",
        "symfony/yaml"                       : "^3.1"
    },
    "prefer-stable"    : true,
    "require-dev"      : {
        "phpunit/phpunit"                  : "^5.1@dev",
        "psy/psysh"                        : "@stable",
        "tracy/tracy"                      : "^2.4@dev",
        "codeclimate/php-test-reporter"    : "dev-master",
        "rdlowrey/auryn"                   : "^1.4",
        "pimple/pimple"                    : "^3.0",
        "illuminate/container"             : "^5.2",
        "phpmd/phpmd"                      : "^2.4",
        "php-di/php-di"                    : "^5.3",
        "monii/container-interop-laravel": "^1.0"
    }
}
