{
    "name"             : "oddgreg/nine-loaders",
    "description"      : "Container-Agnostic Configuration Loaders and Sets.",
    "type"             : "library",
    "license"          : "MIT",
    "authors"          : [
        {
            "name" : "Greg Truesdell",
            "email": "odd.greg@gmail.com"
        }
    ],
    "repositories"     : [
        {
            "url" : "https://github.com/OddGreg/NineDI.git",
            "type": "vcs"
        }
    ],
    "autoload"         : {
        "classmap"             : [
            "tests",
            "src"
        ],
        "files"                : [
            "src/Support/helpers.php"
        ],
        "exclude-from-classmap": [
        ],
        "psr-4"                : {
            "Nine\\Loaders\\": "src"
        }
    },
    "require"          : {
        "php"                                : ">=7.0",
        "oddgreg/nine-library"               : "dev-master",
        "container-interop/container-interop": "^1.1",
        "symfony/yaml"                       : "2.8.*|3.0.*",
        "symfony/finder"                     : "2.8.*|3.0.*"
    },
    ".require-dev"      : {
        "oddgreg/nine-di"             : "dev-master",
        "phpunit/phpunit"             : "^5.1@dev",
        "psy/psysh"                   : "@stable",
        "tracy/tracy"                 : "^2.4@dev",
        "symfony/dependency-injection": "3.1.*"
    },
    "minimum-stability": "dev",
    "prefer-stable"    : true
}
